
# ENVIRONMENT SWITCHER

# Client-side switcher (safe for browser)
# Set to true for production Supabase, false for local Docker
NEXT_PUBLIC_PROD_DB=false

# Server-side switcher (not exposed to client)
# Should match NEXT_PUBLIC_PROD_DB value
PROD_DB=false

# Note: NODE_ENV is automatically set by Next.js
# - 'development' when running `next dev`
# - 'production' when running `next build` and `next start`
# Do not set this manually in .env files


# PRODUCTION DATABASE (Supabase Cloud)
# ⚠️ SERVER-SIDE ONLY - These are secrets and must NOT have NEXT_PUBLIC_ prefix

PROD_DATABASE_URL=
PROD_SUPABASE_URL=
PROD_SUPABASE_SERVICE_ROLE_KEY=
PROD_SUPABASE_ANON_KEY=

# ✅ CLIENT-SIDE SAFE - Production variables for browser
NEXT_PUBLIC_PROD_SUPABASE_URL=
NEXT_PUBLIC_PROD_SUPABASE_ANON_KEY=


